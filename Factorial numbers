import java.util.Scanner;
class demo
{
	int i,n,fact=1;
	Scanner s=new Scanner(System.in);
	void get()
	{
		System.out.println("enter the number");
		n=s.nextInt();
	}
	void display()
	{
		for(i=1;i<=n;i++)
		{
			fact=fact*i;
		}
		System.out.println("factorinal num"+n+"is"+fact);
	}
}
class fact
{
		public static void main(String args[])
		{
			demo d=new demo();
			d.get();
			d.display();
		}
}
		
