import java.util.Scanner;
class prime
{
    void put()
    {
        int num,i,count=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number");
        num=sc.nextInt();
        for(i=2;i<num;i++)
        {
            if(num%i==0)
            {
                count++;
                break;
            }
        }
        if(count==0)
        System.out.println("number is prime");
        else
        System.out.println("number is not prime");
    }
}
class demo
{
    public static void main(String args[])
    {
        prime p=new prime();
        p.put();
    }
}
